

{
    "component": "SponserRegistration",
    "form": {
      "title": "Sponsor Registration",
      "fields": [
        {
          "label": "Name",
          "type": "text",
          "id": "name",
          "placeholder": "Enter your name",
          "required": true
        },
        {
          "label": "Email address",
          "type": "email",
          "id": "email",
          "placeholder": "Enter your email",
